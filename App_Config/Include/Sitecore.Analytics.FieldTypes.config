<?xml version="1.0" encoding="utf-8" ?>
<!-- Purpose: This include file configures fieldType in the Analytics subsystem. 
       Note: some fields are only defined in the core database under: /sitecore/system/Field types
  -->
<configuration xmlns:role="http://www.sitecore.net/xmlconfig/role/" xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore role:require="Standalone OR ContentDelivery OR ContentManagement OR Processing OR Reporting">
     <fieldTypes>
      <fieldType name="Tracking" type="Sitecore.Analytics.Data.TrackingField,Sitecore.Analytics" resizable="true" />
    </fieldTypes>
  </sitecore>
</configuration>