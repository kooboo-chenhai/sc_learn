<?xml version="1.0" encoding="utf-8" ?>
<!--
    
Purpose: Purpose: This include file defines a number of processors for the pipelines that are executed when the system indexes experience data.
   
-->
<configuration xmlns:role="http://www.sitecore.net/xmlconfig/role/" xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore role:require="Standalone OR ContentDelivery OR ContentManagement OR Processing OR Reporting">
    <pipelines>
      <contactindexable.loadfields>
        <!-- Populates collection of Facebook fields for indexing. -->
        <processor type="Sitecore.Social.Facebook.Connector.ContentSearch.Analytics.Pipelines.ContactIndexableLoadFields.LoadFacebookFields, Sitecore.Social.Facebook" />
      </contactindexable.loadfields>
    </pipelines>
  </sitecore>
</configuration>
